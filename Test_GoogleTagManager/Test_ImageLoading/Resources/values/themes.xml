<?xml version="1.0" encoding="UTF-8"?>
<resources>

	<style name="Theme.ForcedUpdate" parent="android:Theme">
		<!--<item name="android:windowBackground">@drawable/forcedUpdate</item>-->
		<item name="android:windowNoTitle">true</item>
	</style>

	 <style name="Theme.NoAnimation" parent="android:Theme">
   		<item name="android:windowAnimationStyle">@null</item>
		<item name="android:windowNoTitle">true</item>
	</style>

	<style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<item name="android:windowBackground">@color/background_window</item>
		<item name="colorPrimary">#4E5152</item>
		<item name="colorPrimaryDark">#689F38</item>
		<item name="colorAccent">#689F38</item>
		<item name="android:textColorPrimary">@android:color/white</item>
		<item name="android:windowFullscreen">true</item>
	</style>

	<style name="Theme.AppCompat.Dark.NoActionBar" parent="AppTheme">
		<item name="android:windowBackground">@color/background_window</item>
		<item name="colorPrimary">#FFFFFF</item>
		<item name="colorPrimaryDark">#689F38</item>
		<item name="colorAccent">#689F38</item>
		<item name="android:textColorPrimary">#11257F</item>
		<item name="android:typeface">serif</item>
	</style>

	<style name="Theme.AppCompat.Dark.NoActionBarTransparent" parent="AppTheme">
		<item name="android:windowBackground">@color/background_window</item>
		<item name="android:background">@android:color/transparent</item>
	</style>

	<style name="Theme.AppCompat.Dark.NoActionBarTransparent.NoAnimation" parent="AppTheme">
		<item name="android:windowBackground">@color/background_window</item>
		<item name="android:background">@android:color/transparent</item>
   		<item name="android:windowAnimationStyle">@null</item>
	</style>

	<style name="AppTheme" parent="AppBaseTheme">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFullscreen">true</item>
		<item name="android:windowActionBar">false</item>
	</style>

	<style name="EditText_MyLayout" parent="TextAppearance.AppCompat">
 	</style>

	<style name="error_appearance" parent="TextAppearance.AppCompat">
	</style>

	  <style name="Theme.Transparent" parent="AppTheme">
	    <item name="android:windowIsTranslucent">true</item>
	    <item name="android:windowBackground">@android:color/transparent</item>
	    <item name="android:windowContentOverlay">@null</item>
	    <item name="android:windowNoTitle">true</item>
	   <!--- <item name="android:windowIsFloating">true</item>-->
	    <item name="android:backgroundDimEnabled">false</item>
	  </style>

</resources>